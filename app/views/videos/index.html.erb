          <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a></p>
        </div>
      </div>
        
        <div class=" container col-md-3 col-xs-3 col-sm-3 menu">
          <form class="navbar-form navbar-left" role="search" id= "search">
            <div class="form-group">
              <input type="text" class="form-control" placeholder="Ex. Moles">
             </div> 
             <button type="submit" class="btn" id= "vidsearch">Search</button>
          </form>
        </div>
        
    </div>
</div>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Link</th>
      <th>Description</th>
      <th>Specialization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= video.title %></td>
        <td><%= video.link %></td>
        <td><%= video.description %></td>
        <td><%= video.specialization.name if video.specialization%></td>
        <td><%= link_to 'Show', video %></td>
      </tr>
    <% end %>
  </tbody>
</table>

